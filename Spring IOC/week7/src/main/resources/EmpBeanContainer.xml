<?xml version="1.0" encoding="UTF-8"?>  
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd"><context:annotation-config />
	
	<bean id="emp1" class="week7.Employee">
		<property name="code" value="E01"></property>
		<property name="name" value="Sachin"></property>
		<property name="age" value="51"></property>
	</bean>
	
	<bean id="emp2" class="week7.Employee">
		<property name="code" value="E02"></property>
		<property name="name" value="Sourav"></property>
		<property name="age" value="57"></property>
	</bean>


	<bean id="emp3" class="week7.Employee">
		<constructor-arg index="0" type="String" value="E03"/>
		<constructor-arg index="1" type="String" value="Virat"/>
		<constructor-arg index="2" type="int" value="31"/>	
	</bean>
	
</beans>